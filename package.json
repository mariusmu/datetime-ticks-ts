{
    "name": "ticks-converter",
    "version": "0.0.4",
    "main": "main.js",
    "author": {
        "email": "mariussmk@gmail.com",
        "name": "Marius Munthe-Kaas"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mariusmu/datetime-ticks"
    },
    "description": "Electron App to make the conversion between C# DateTime ticks and JS-ticks easy",
    "scripts": {
        "web": "rollup -cw --",
        "lint": "tslint --project tsconfig.json --type-check",
        "watch": "SET NODE_ENV=development && concurrently \"rollup -cw\"",
        "build": "cross-env NODE_ENV=production rollup -c --environment BUILD:production",
        "index": "cp index.html build/index.html && cp bundle.js build/bundle.js",
        "publish": "npm run build && npm run index && build --win -p always"
    },
    "devDependencies": {
        "@types/react": "16.7.18",
        "@types/react-dom": "16.0.11",
        "@types/react-flatpickr": "^3.6.0",
        "@types/react-hot-loader": "4.1.0",
        "@types/react-router-dom": "4.3.1",
        "@types/styled-components": "4.1.4",
        "concurrently": "4.1.0",
        "moment": "^2.23.0",
        "react-flatpickr": "^3.6.4",
        "rollup": "1.1.0",
        "rollup-plugin-commonjs": "9.2.0",
        "rollup-plugin-livereload": "0.6.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-node-resolve": "4.0.0",
        "rollup-plugin-postcss": "^1.6.3",
        "rollup-plugin-replace": "2.1.0",
        "rollup-plugin-serve": "1.0.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-typescript": "1.0.0",
        "rollup-plugin-uglify": "6.0.0",
        "rollup-watch": "4.3.1",
        "tslib": "1.9.3",
        "typescript": "3.2.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.12",
        "@fortawesome/free-solid-svg-icons": "^5.6.3",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "cross-env": "^5.2.0",
        "devicon-react": "1.1.1",
        "font-awesome": "4.7.0",
        "history": "4.7.2",
        "react": "16.7.0",
        "react-datetime": "^2.16.3",
        "react-devicon": "^0.1.9",
        "react-dom": "16.7.0",
        "react-hot-loader": "4.6.3",
        "react-router-dom": "4.3.1",
        "rollup-plugin-svg": "^1.0.1",
        "source-map-support": "0.5.9",
        "styled-components": "4.1.3",
        "tslint": "5.12.0"
    }
}
